{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/Conversion",
    "definitions": {
        "Conversion": {
            "properties": {
                "id": {
                    "$ref": "#/definitions/mlayer.Identifier",
                    "additionalProperties": true
                },
                "src_id": {
                    "$ref": "#/definitions/mlayer.Identifier",
                    "additionalProperties": true
                },
                "dst_id": {
                    "$ref": "#/definitions/mlayer.Identifier",
                    "additionalProperties": true
                },
                "scaletype": {
                    "enum": [
                        "NONE",
                        0,
                        "RATIO",
                        1,
                        "INTERVAL",
                        2,
                        "ORDINAL",
                        3,
                        "BOUNDED",
                        4
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Scale Type"
                },
                "factors": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Conversion",
            "description": "Conversion between two scales of the same type with the same aspect"
        },
        "mlayer.Identifier": {
            "properties": {
                "name": {
                    "type": "string"
                },
                "value": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Identifier"
        }
    }
}